# udev rules for Pixhawk

# Pixhawk Autopilots
# ------------------

# Pixracer (and probably others)
SUBSYSTEM=="tty", ATTRS{manufacturer}=="3D Robotics", ATTRS{product}=="PX4 FMU*", SYMLINK+="ttyPixhawk"


# Radios
# ----------
SUBSYSTEM=="tty", ATTRS{manufacturer}=="Silicon Labs", SYMLINK+="ttySiK"
